{
    "name": "mindtouch-martian",
    "testEnvironment": "node",
    "coverageDirectory": "__coverage-node__",
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
        "api.js": true,
        "contextId.js": true,
        "draft.js": true,
        "draftFile.js": true,
        "file.js": true,
        "group.js": true,
        "learningPath.js": true,
        "page.js": true,
        "pageBase.js": true,
        "pageFile.js": true,
        "pageFileBase.js": true,
        "pageProperty.js": true,
        "site.js": true,
        "user.js": true,
        "events.js": true,
        "workflows.js": true,
        "lib/modelParser.js": true,
        "lib/settings.js": true,
        "lib/tokenHelper.js": true,
        "lib/utility.js": true,
        "lib/propWatcher.js": true
    },
    "automock": true,
    "unmockedModulePathPatterns": [ "<rootDir>/models/" ],
    "testPathIgnorePatterns": [
        "<rootDir>/node_modules/",
        "<rootDir>/models/__tests__/mock/"
    ],
    "moduleNameMapper": {
        "mindtouch-http/plug.js": "<rootDir>/__mocks__/plug.js",
        "mindtouch-http/progressPlugFactory.js": "<rootDir>/__mocks__/progressPlugFactory.js",
        "mindtouch-http/uri.js": "<rootDir>/__mocks__/uri.js",
        "mindtouch-http/lib/uriParser.js": "<rootDir>/__mocks__/uriParser.js"
    }
}
