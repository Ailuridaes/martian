{
    "name": "mindtouch-martian",
    "testEnvironment": "node",
    "coverageDirectory": "__coverage-node__",
    "collectCoverage": true,
    "collectCoverageFrom": [ "*.js", "lib/*.js", "!lib/platform.js", "!lib/tokenHelper.js", "!main.js", "!global.js" ],
    "automock": true,
    "unmockedModulePathPatterns": [ "<rootDir>/models/", "<rootDir>/lib/validation.js" ],
    "testPathIgnorePatterns": [
        "<rootDir>/node_modules/",
        "<rootDir>/models/__tests__/mock/",
        "<rootDir>/__tests__/global.test.js"
    ],
    "moduleNameMapper": {
        "/mindtouch-http.js/plug.js": "<rootDir>/__mocks__/plug.js",
        "/mindtouch-http.js/progressPlug.js": "<rootDir>/__mocks__/progressPlug.js",
        "/mindtouch-http.js/uri.js": "<rootDir>/__mocks__/uri.js",
        "/mindtouch-http.js/lib/uriParser.js": "<rootDir>/__mocks__/uriParser.js"
    }
}
