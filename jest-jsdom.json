{
    "name": "mindtouch-martian",
    "coverageDirectory": "__coverage-jsdom__",
    "collectCoverage": true,
    "collectCoverageFrom": [ "*.js", "lib/*.js", "!lib/platform.js", "!lib/dispatchEvent.js" ],
    "automock": true,
    "unmockedModulePathPatterns": [ "<rootDir>/models/" ],
    "testPathIgnorePatterns": [
        "<rootDir>/node_modules/",
        "<rootDir>/models/__tests__/mock/"
    ],
    "moduleNameMapper": {
        "mindtouch-http/plug.js": "<rootDir>/__mocks__/plug.js",
        "mindtouch-http/progressPlug.js": "<rootDir>/__mocks__/progressPlug.js",
        "mindtouch-http/uri.js": "<rootDir>/__mocks__/uri.js",
        "mindtouch-http/lib/uriParser.js": "<rootDir>/__mocks__/uriParser.js"
    }
}
